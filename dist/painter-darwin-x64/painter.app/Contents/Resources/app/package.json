{
  "name": "painter",
  "version": "1.0.0",
  "description": "you are your painter",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=dist --icon=app-icon/mac/app.icns",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=dist --icon=app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=dist"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.1",
    "electron-packager": "^13.0.1"
  }
}
